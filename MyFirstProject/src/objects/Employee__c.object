    <validationRules>
        <fullName>AgeBetween21To30</fullName>
        <active>true</active>
        <description>Error if age is not between 21 to 30</description>
        <errorConditionFormula>OR( Age__c &lt; 21, Age__c &gt; 30)</errorConditionFormula>
        <errorDisplayField>Date_Of_Birth__c</errorDisplayField>
        <errorMessage>Please provide a valid Date of birth. Employee Age should be between 21 to 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Joining_date_validation</fullName>
        <active>true</active>
        <description>Joining date cannot be greater then last working date</description>
        <errorConditionFormula>Date_of_Joining__c  &gt;  Last_Working_Day__c</errorConditionFormula>
        <errorDisplayField>Date_of_Joining__c</errorDisplayField>
        <errorMessage>Date of joining cannot be greater than Last working date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_validation</fullName>
        <active>true</active>
        <description>If person is on notice period then resignation date cannot be null</description>
        <errorConditionFormula>IF( Is_on_Notice_Period__c == true &amp;&amp;   ISNULL( Resignation_Date__c )  ,true,false)</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Resignation date cannot be null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSNSizeLimit</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(TEXT( SSN__c ) ) &lt;&gt; 10</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>Length should be 10</errorMessage>
    </validationRules>
